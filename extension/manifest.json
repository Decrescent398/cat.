{
    "manifest_version": 3,
    "name": "cat.",
    "version": "1.0",
    "description": "meow.",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
    "default_popup": "popup.html",
    "default_icon": "images/cat.png"
    },
    "icons": {
      "48": "images/cat.png"
    }
  }
  