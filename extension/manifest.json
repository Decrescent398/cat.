{
    "manifest_version": 3,
    "name": "Cat.",
    "version": "1.0",
    "description": "Meow.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "images/cat.png"
    },
    "icons": {
      "48": "images/cat.png"
    }
  }
  